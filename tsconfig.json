{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"],
    },
    "jsx": "preserve",
  },
}
